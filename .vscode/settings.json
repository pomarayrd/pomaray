{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit"
	},
	"files.associations": {
		"*.mdx": "markdown"
	},

	"cSpell.language": "en,es-ES",
	"cSpell.enabled": true,
	"cSpell.words": [
		"pomaray",
		"Pomaray",
		"Rafaela",
		"Rafela",
		"Ybarra"
	]
}
